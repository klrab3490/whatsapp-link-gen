{
    "manifest_version": 3,
    "name": "WhatsApp Link Generator",
    "version": "1.0.3",
    "description": "Generate WhatsApp links for payment requests",
    "permissions": ["activeTab", "tabs","storage"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon.png",
            "48": "icon.png",
            "128": "icon.png"
        }
    },
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    }
}
  